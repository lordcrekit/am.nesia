<!DOCTYPE html>
<html>
<head>
    <script src="https://cdn.auth0.com/js/lock/10.3/lock.min.js"></script>
</head>
<body>
    <script>
        var lock = new Auth0Lock(
                '<%= env.AUTH0_CLIENT_ID%>',
                '<%=env.AUTH0_DOMAIN%>',
                { auth: {
                    redirectUrl: '<%=env.AUTH0_CALLBACK_URL%>',
                    responseType: 'code',
                    params: {
                        scope: 'openid name email picture'
                    }
                }});
        lock.show();
    </script>
</body>
</html>
